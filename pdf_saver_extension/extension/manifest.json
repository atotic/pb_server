{
  "name": "pdf_saver_extension",
  "version": "1.0",
  "description": "PDF conversion with Chrome",
  "background_page": "background.html",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon.png",
	"default_title": "save as PDF on server"
  },
  "permissions": [
	 "tabs",
	 "pageCapture",
	 "notifications",
	 "http://*/",
	 "webNavigation",
	 "<all_urls>"
  ]
}